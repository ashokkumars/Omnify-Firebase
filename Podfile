  # Pods for Omnify Firebase

  target 'Omnify FirebaseTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'Omnify FirebaseUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
